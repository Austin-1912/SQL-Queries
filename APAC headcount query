Total Sent by Keyword = 
VAR SelectedKeywords = VALUES('Campaign Keywords'[Keyword])
RETURN
    CALCULATE(
        SUM(SJ_BI_CAMPAIGN_COMMUNICATIONS_2_V2_MYWU_UPDATE[COMMUNICATIONS_SENT]),
        FILTER(
            SJ_BI_CAMPAIGN_COMMUNICATIONS_2_V2_MYWU_UPDATE,
            VAR CampaignName = SJ_BI_CAMPAIGN_COMMUNICATIONS_2_V2_MYWU_UPDATE[CAMPAIGN_NAME]
            RETURN
                COUNTROWS(
                    FILTER(
                        SelectedKeywords,
                        CONTAINSSTRING(UPPER(CampaignName), UPPER([Keyword]))
                    )
                ) > 0
        )
    )
 
Total Opened by Keyword = 
VAR SelectedKeywords =
    VALUES ( 'Campaign Keywords'[Keyword] )
RETURN
CALCULATE (
    SUM ( SJ_BI_CAMPAIGN_COMMUNICATIONS_2_V2_MYWU_UPDATE[COMMUNICATIONS_OPENED_ATLEAST_ONCE_WITHIN_7_DAYS_OF_COMMUNICATION] ),
    FILTER (
        SJ_BI_CAMPAIGN_COMMUNICATIONS_2_V2_MYWU_UPDATE,
        VAR CampaignName =
            UPPER ( SJ_BI_CAMPAIGN_COMMUNICATIONS_2_V2_MYWU_UPDATE[CAMPAIGN_NAME] )
        RETURN
            COUNTROWS (
                FILTER (
                    SelectedKeywords,
                    CONTAINSSTRING ( CampaignName, UPPER ( 'Campaign Keywords'[Keyword] ) )
                )
            ) > 0
    )
)

Total Clicked by Keyword = 
VAR SelectedKeywords = VALUES('Campaign Keywords'[Keyword])
RETURN
    CALCULATE(
        SUM(SJ_BI_CAMPAIGN_COMMUNICATIONS_2_V2_MYWU_UPDATE[COMMUNICATIONS_WITH_ATLEAST_ONE_LINK_CLICK_WITHIN_7_DAYS_OF_COMMUNICATION]),
        FILTER(
            SJ_BI_CAMPAIGN_COMMUNICATIONS_2_V2_MYWU_UPDATE,
            VAR CampaignName = SJ_BI_CAMPAIGN_COMMUNICATIONS_2_V2_MYWU_UPDATE[CAMPAIGN_NAME]
            RETURN
                COUNTROWS(
                    FILTER(
                        SelectedKeywords,
                        CONTAINSSTRING(UPPER(CampaignName), UPPER([Keyword]))
                    )
                ) > 0
        )
    )
