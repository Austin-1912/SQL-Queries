select
    '@' || t.txn_id AS TXN_ID,
    MIN(rcvpaying_datetime) as transaction_date,
    '@' || t.sndcustomer_key as GID,
    t.send_country_name,
    t.Send_state,
    c.country_code As Registered_Country_Code,
    c.state as Registered_State,
    c.city as Registered_City,
    RCVCOUNTRY_CODE as corridor,
    t.SND_Purpose_Purchase_Category as Transaction_Reason,
        MONTH(rcvpaying_datetime) as transaction_Month,
    YEAR(rcvpaying_datetime) as transaction_Year,
FROM WUDNA.SUMMARY_GEN.WUDNA_CUSTOMER_MASTER_VW cm
LEFT JOIN wudna.summary_gen.wudna_txn_master_analytics_vw txn
where cm.first_registration_date >= '2025-12-01' 
and cm.FIRST_REGISTRATION_DATE <= '2025-12-31' 
and cm.country_code = 'IN'
