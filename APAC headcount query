select
   '@' || t.SNDCUSTOMER_KEY AS GID,
    '@' || c.customer_umn AS UMN,
    t.SEND_COUNTRY_NAME,
    t.RCV_COUNTRY_NAME,
    count(t.txn_id)
from
    CAR_645_AU_N7_PRICING_TXN_DETAILS t
LEFT JOIN WUDNA.SUMMARY_GEN.WUDNA_CUSTOMER_MASTER_VW c
    ON t.SNDCUSTOMER_KEY = c.CUSTOMER_KEY
where
    RCVCOUNTRY_CODE in ('PK','NP','CO')
group by ALL;
