Select
    a.MTH_YR,
    a.SNDCUSTOMER_KEY,
    a.COUNTRY_CODE,
     a.SEND_COUNTRY_NAME,
     a.R1_TXNS,
    a.R1_APP_TXNS,
    a.R1_WEBSITE_TXNS,
    a.R1_PRINCIPAL,
    a.R1_REVENUE,
FROM CAR_717_Customer_Channel_Monthly1
    JOIN WUDNA.SUMMARY_GEN.WUDNA_TXN_MASTER_ANALYTICS_VW b
    ON a.SNDCUSTOMER_KEY = b.SNDCUSTOMER_KEY
    AND a.MTHYR = b.DATE_TRUNC('MONTH', b.RCVPAYING_DATETIME)
    DATE_TRUNC('MONTH', b.RCVPAYING_DATETIME) <= a.MTHYR AND DATE_TRUNC('MONTH', b.RCVPAYING_DATETIME) > DATEADD(MONTH, -12, a.MTHYR)
